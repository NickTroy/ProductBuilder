<script type="text/javascript">
  ShopifyApp.ready(function(){
    ShopifyApp.Bar.initialize({
      title: "Home"
      });
  });
</script>
<h3>Products</h3>
<table>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td>
          <%= product.id %>
        </td>
        <td>
          <%= product.title %>
        </td>
        <td>
          <%= product.image %>
        </td>
        <td>
          Product has <%= product.variants.count %> variants
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'All Products', products_path %>
