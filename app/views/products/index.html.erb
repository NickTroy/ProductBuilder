<tbody>
  <% @products.each do |product| %>
    <tr>
      <td>
        <%= product.id %>
      </td>
      <td>
        <%= product.title %>
      </td>
      <td>
        <%= product.image %>
      </td>
      <td>
        Product has <%= product.variants.count %> variants
      </td>
    </tr>
  <% end %>
</tbody>

<%= link_to 'All Products', products_path %>